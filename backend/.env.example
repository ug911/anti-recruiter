# Zoho Recruit OAuth Credentials
# Obtain these from the Zoho API Console: https://api-console.zoho.in/
ZOHO_CLIENT_ID=YOUR_ZOHO_CLIENT_ID
ZOHO_CLIENT_SECRET=YOUR_ZOHO_CLIENT_SECRET

# Ensure this matches the redirect URI registered in Zoho API Console
ZOHO_REDIRECT_URI=http://localhost:8000/auth/zoho/callback

# System-level Zoho Refresh Token for Backend-only Auth
ZOHO_REFRESH_TOKEN=YOUR_REFRESH_TOKEN_HERE

# Database Configuration
DATABASE_URL=postgresql://user:password@host/dbname?sslmode=require
